{
  "name": "@highbeam/interface",
  "version": "0.1.0",
  "private": true,
  "main": "build/index.js",
  "dependencies": {},
  "scripts": {
    "build": "npm-run-all --parallel build:source build:declaration",
    "build:source": "swc src -d build",
    "build:declaration": "tsc",
    "postinstall": "yarn build",
    "dev": "nodemon -e ts --watch ./src --exec \"yarn build\""
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4"
  }
}

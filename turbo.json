{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "build/**"]
    },
    "bundle:mac": {},
    "bundle:win": {},
    "bundle": {
      "dependsOn": ["bundle:mac", "bundle:win"],
      "outputs": ["dist/**", ".next/**"]
    }
  }
}
